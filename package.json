		"test": "npm run lint && npm run build",
		"compile": "npm run build",
		"release": "np --no-yarn --no-tests --no-cleanup --no-publish",
		"dev": "nodemon",
		"start": "node dist/server.js"
	},
	"dependencies": {},
	"keywords": [],
	"engines": {
		"node": "^12.17.0 || ^14.13 || >=16.0.0"
